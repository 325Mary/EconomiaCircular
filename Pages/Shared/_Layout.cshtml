<!DOCTYPE html>
<html lang="es">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Economía Circular AMVA</title>

    <!-- CSS -->
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/header.css" asp-append-version="true" />
        <link rel="stylesheet" href="~/css/hoja-de-ruta.css" asp-append-version="true" />
        <link rel="stylesheet" href="~/css/acompanamientos.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/footer.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/sectores.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/indicadores.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/normativa.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/contacto.css" asp-append-version="true" />
    @RenderSection("Styles", required: false)
</head>
<body>
    <!-- Header -->
    <partial name="_Header" />

    <!-- Main Content -->
    <main role="main">
        @RenderBody()
    </main>

    <!-- Footer -->
    <partial name="_Footer" />

    <!-- Scripts -->
    <script src="~/js/header.js" asp-append-version="true"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/js/sectores.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
